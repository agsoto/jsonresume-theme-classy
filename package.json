{
  "name": "@jsonresume/jsonresume-theme-class",
  "version": "0.5.1",
  "description": "Class Theme for JSON Resume",
  "author": "Seth Falco",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "resume",
    "cv",
    "handlebars",
    "jsonresume",
    "jsonresume-theme",
    "jsonresume-theme-class"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jsonresume/jsonresume-theme-class"
  },
  "files": [
    "dist/index.js",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "vite build",
    "serve": "npm run build && resume serve --theme . --resume ./test/fixture.resume.json",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "tsc --noEmit",
    "generate-preview": "npm run build && node scripts/generate-preview.js",
    "build:pdf": "npm run build && resume export --theme . --resume ./test/fixture.resume.json resume.pdf",
    "build:html": "npm run build && resume export --theme . --resume ./test/fixture.resume.json resume.html"
  },
  "engines": {
    "node": ">=v20.11"
  },
  "dependencies": {
    "@fluent/bundle": "^0.19.1",
    "@fluent/langneg": "^0.7.0",
    "handlebars": "^4.7.8",
    "html-minifier": "^4.0.0",
    "marked": "^16.4.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/html-minifier": "^4.0.5",
    "@types/node": "^24.8.1",
    "eslint": "^9.38.0",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^14.0.0",
    "puppeteer": "^18.2.1",
    "resume-cli": "^3.1.2",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
